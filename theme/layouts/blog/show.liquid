{% include 'head' %}
{% include 'header' %}
<main class="page container">
    <article class="stack post">
        <header class="stack post-header">
            <p class="muted">{{ post.date }}</p>
            <h1 class="post-title">{{ post.frontmatter.title }}</h1>
        </header>
        <div class="post-content prose">
            {{ post.html_content }}
        </div>
    </article>
    <nav class="page-nav">
        <a href="/blog/">&larr; Back to writing</a>
    </nav>
</main>
{% include 'footer' %}
