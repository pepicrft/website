{% include 'head' %}
{% include 'header' %}
<main class="page container">
    <article class="stack note">
        <header class="stack note-header">
            <time datetime="{{ note.date }}" class="muted">{{ note.date }}</time>
        </header>
        <div class="note-content prose">
            {{ note.html_content }}
        </div>
    </article>
    <nav class="page-nav">
        <a href="/notes/">&larr; Back to notes</a>
    </nav>
</main>
{% include 'footer' %}
